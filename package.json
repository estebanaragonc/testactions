{
  "name": "vb-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "format": "prettier --check --ignore-path .gitignore .",
    "format:prettier": "prettier --write --ignore-path .gitignore .",
    "format:eslint": "eslint --ignore-path .gitignore --fix .",
    "format:fix": "npm run format:prettier && npm run format:eslint",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "sb": "storybook dev --ci -p 6006",
    "docker:build": "docker build --no-cache -f Dockerfile.Qa -t cypress .",
    "docker:test": "npm run docker:build && docker run --rm cypress",
    "test": "npm run docker:test",
    "build-sb": "storybook build",
    "cypress:open": "npx cypress open",
    "cypress:run": "npx cypress run",
    "prebrowserstack": "node ./cypress/utils/generateBrowserstackConfig.ts",
    "browserstack": "browserstack-cypress run",
    "to-merge": "npx jrm 'cypress/reports/combined.xml' 'build_artifacts/**/*.xml'"
  },
  "precommit": [
    "format",
    "lint"
  ],
  "dependencies": {
    "@commercetools/platform-sdk": "^4.7.1",
    "@commercetools/sdk-client-v2": "^2.1.6",
    "@contentstack/utils": "^1.3.0",
    "@cypress/grep": "^3.1.5",
    "@types/node": "18.15.12",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "class-variance-authority": "^0.6.0",
    "contentstack": "^3.17.1",
    "cva": "npm:class-variance-authority@^0.6.0",
    "next": "13.3.0",
    "node-fetch": "^3.3.1",
    "path": "^0.12.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.1.5",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/addon-styling": "^1.0.6",
    "@storybook/blocks": "^7.0.6",
    "@storybook/manager-api": "^7.0.6",
    "@storybook/nextjs": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/theming": "^7.0.6",
    "@types/fs-extra": "^11.0.1",
    "@types/xmldom": "^0.1.31",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "autoprefixer": "^10.4.14",
    "browserstack-cypress-cli": "^1.22.1",
    "cypress": "^12.10.0",
    "cypress-axe": "^1.4.0",
    "cypress-xpath": "^2.0.1",
    "cypress-xray-plugin": "^3.2.0",
    "eslint": "^8.40.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.4.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "fs-extra": "^11.1.1",
    "junit-report-merger": "^6.0.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "postcss": "^8.4.23",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.7",
    "storybook": "^7.0.6",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4"
  }
}
